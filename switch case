<script>
var pilihan =  prompt ("harga obatan (1-5):");
var obat="";
switch (obat) {
  case '1':
    obat = "paracetamol 500mg " ;
    break;
  case '2':
    obat = " Ibu Profen" ;
    break;
  case '3':
    obat = "Aspirin" ;
    break;
  case '4':
    obat = "Met Actoplus" ;
    break;
  case '5':
    obat = "Sirup diare" ;
    break;
  default:
    obat=" ";
  }
 if (obat == ""){
     document.write("pilihan obat anda tidak tepat")
 }else {
     document.write(" obat yang anda pilih adalah" + obat +"</h2>");
 }
</script>

